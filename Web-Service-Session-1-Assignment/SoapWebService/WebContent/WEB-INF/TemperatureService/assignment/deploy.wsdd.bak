<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TemperatureService WSDL service -->

  <service name="Temperature" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://assignment"/>
      <parameter name="wsdlServiceElement" value="TemperatureService"/>
      <parameter name="schemaQualified" value="http://assignment"/>
      <parameter name="wsdlServicePort" value="Temperature"/>
      <parameter name="className" value="assignment.TemperatureSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Temperature"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="convertFahrenheitToCelsius" qname="operNS:convertFahrenheitToCelsius" xmlns:operNS="http://assignment" returnQName="retNS:convertFahrenheitToCelsiusReturn" xmlns:retNS="http://assignment" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:temperatureInFahrenheit" xmlns:pns="http://assignment" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="convertFahrenheitToCelsius"/>

  </service>
</deployment>
